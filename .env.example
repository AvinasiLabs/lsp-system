# PostgreSQL数据库配置
POSTGRES_DBNAME=mydatabase
POSTGRES_USER=myuser
POSTGRES_PWD=mypassword
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# API配置
# 认证系统开关（true/false）
API_AUTH_ENABLED=false

# JWT配置（仅在API_AUTH_ENABLED=true时使用）
API_JWT_SECRET_KEY=your-secret-key-here-change-this-in-production
API_JWT_ALGORITHM=HS256
API_JWT_EXPIRE_MINUTES=1440

# 服务器配置
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# CORS配置（生产环境请修改）
API_CORS_ORIGINS=["*"]
API_CORS_ALLOW_CREDENTIALS=true
API_CORS_ALLOW_METHODS=["*"]
API_CORS_ALLOW_HEADERS=["*"]